import java.util.Scanner;
public class Main{
   public static void main(String[] args){
   Scanner sc=new Scanner(System.in);
   int a=sc.nextInt();
   int b=sc.nextInt();
   int x=a;
   int y=b;
   int temp;
    while(y!=0){
    temp=y;
    y=x%y;
    x=temp;}
    System.out.println(x);
    int gcd = x;
    int lcm= (a*b)/gcd;
    System.out.println(lcm);
    }

}
